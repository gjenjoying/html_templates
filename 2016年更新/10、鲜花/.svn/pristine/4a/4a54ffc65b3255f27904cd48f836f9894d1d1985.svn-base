body{font-family: "微软雅黑";overflow-x: hidden;}


i,em{font-style: normal;}

img{
    max-width: 100%;
}
    
 ul li{list-style: none;}
.vcenter {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    max-width: 100%;
    max-height: 100%;
    display: block;
    vertical-align: middle;
    margin: auto;
}

.tHide{
    text-overflow:ellipsis;
    overflow: hidden;
    white-space:nowrap;
}

.blockFull {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
}
    
.relative{
    position: relative;
}
    
.css3Tansition(@time:0.3s,@animation:ease-out) {
    transition:All @time @animation;
}
    
a{
    color: #666;
}
    
a:hover{
    color:@sub_color;
    text-decoration: none;
}
    
h1,h2,h3,h4,h5,h6,ul,li{
    margin: 0;
    padding: 0;
}

img{
    border: 0;
}



