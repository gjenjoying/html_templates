     b{font-size: 18px;color: #000;}
 }
 .contactimg{
     ul li{display: inline-block;width: 10%;}
     .pic{
         img{padding: 10px;border: 1px solid #dbdbdb;}
         &:after{padding-bottom: 86%;}
     }
     .contactbg{width: 60px;height: 60px;border-radius: 50%;background: @theme_color url(../images/contactbg.png) no-repeat center;margin: 20px auto;}
 }
.contact-con{padding-bottom: 115px;border-bottom: 1px solid #d9d9d9;
    ul{border: 1px solid #000;border-right: 0;height: 240px;}
    ul li{width: 25%;border-right: 1px solid #000;text-align: center;color: #000;height: 100%;padding:50px 60px 30px; float: left;
        .pic{
            &:after{padding-bottom:25% ;}
        }
        b{font-family: arial;font-size: 16px;display: block;}
        span{font-size: 18px;display: block;margin-top: 10px;}
        em{font-size: 22px;display: block;
            a{color: #000;transition: 0.3s;
                &:hover{color: @sub_color;}
            }
        }
    }
}
 
 
 //品牌简介
.brand-con{text-align: center;color: #000;border-bottom: 1px solid #000;padding-bottom: 30px;
    b{font-size: 26px;line-height: 30px;display: block;margin: 50px 0 0;}
    span{display: block;font-size: 22px;line-height: 60px;}
    .brandbg{width: 60px;height: 60px;margin: 30px auto 50px;border-radius: 50%;background: @sub_color url(../images/contactbg.png) no-repeat center;}
    h3{font-size: 30px;font-weight: bold;margin: 30px 0;}
    .pic{margin: 50px 0;
        &:after{padding-bottom: 32%;}
    }
} 
 
//人才招聘
.job-title{line-height: 60px;background: #e95c5c;font-size: 16px;color: #fff;
    b.zhiwei{margin: 0 21.5% 0 5%;}
    b.jobadd{margin: 0 10% 0 10%;}
    b.caozuo{margin-left: 25%;}
}
.job-con{color: #000;padding-bottom: 100px;border-bottom: 1px solid #000;
ul li{line-height: 60px;border-bottom: 1px solid #cdcdcd;
    .jobconh{cursor: pointer;}
}
li.active{border-bottom: 0;}
li.active .zhiwei{color: @sub_color;}
    b.zhiwei{margin: 0 9% 0 5%;min-width:17% ;display: inline-block;}
    span.jobadd{margin: 0 10% 0 11.5%;}
    span.date{margin: 0 10% 0 0.5%;}
    span.click{margin-left: 15%;}
    
    .job-conwrap{color: #666;width:95%;border: 1px solid #dbdbdb;margin: 30px auto 15px;padding: 40px 0 30px 30px;display: none;position: relative;
        img{position: absolute;top: -12px;left: 55px;}
        dl{float: left;margin-right: 50px;}
        dt{font-size: 16px;}
        dd{font-size: 12px;line-height: 30px;}
    }
}
 
//在线留言
.messageh{text-align: center;margin: 30px 0 20px;
    h2{font-size: 36px;font-weight: bold;color: #000;}
    span{font-weight: bold;font-size: 16px;color: #000;line-height: 50px;}
} 
.message-con{padding-bottom: 100px;border-bottom: 1px solid #000;
    .pic{width: 35%;
        &:after{padding-bottom: 85%;}
    }
    .mcon-right{width: 60%;padding-top: 40px;
    span{display: block;font-weight: 100;color: #545454;}
    label{display: inline-block;margin: 10px 0;position: relative;
        em{position: absolute;right: 50px;top: 30px;color: #f00;}
    }
    label.mcodela{margin-top: 30px;
        em{right: 62%;top: 15px;}
        input{width: 170px;float: left;margin-right: 20px;}
        img{cursor: pointer;float: left;}
        a{margin: 20px 0 0 20px;float: left;text-decoration: underline;font-weight: 100;color: #999;transition: 0.3s;
            &:hover{color: @sub_color;}
        }
    }
    input{width: 400px;line-height: 42px;height: 42px;border: 1px solid #dbdbdb;background: #f7f7f7;margin-right: 40px;text-indent: 5px;}
    textarea{width: 845px;border: 1px solid #dbdbdb;background: #f7f7f7;height: 120px;text-indent: 5px;margin-right: 40px;}
    .msub{width: 205px;height: 42px;float: right;margin-left: 216px;border: 0;background: #e60012;color: #fff;font-weight: 100;}
    }
}
 
 
.map { margin: 40px auto;border-bottom: 1px solid @theme_color;padding-bottom: 100px;}
.map dl { margin:20px auto 20px;overflow: hidden;line-height:30px;width: 98%;transition: 0.3s;}
.map dl:hover{box-shadow: 0 0 2px #000;}
.map dl:hover dt a {color:#333; font-weight: bold;}

.map dt { font-size:14px;float: left; width: 10%; text-align: center; border: 1px solid @navline_color;transition: 0.3s;}
.map dt:hover { background: #000;}
.map dt:hover a{color: #fff !important;}
.map dt a { color:#000; display: block;padding: 10px 0; transition: 0.3s;}
.map dt a:hover { color:#e0e9f3; font-weight: bold;}

.map dd {font-size:14px;  overflow:hidden; float: left; width: 85%;line-height: 40px;}
.map dd .p2 { padding:0 0 0 20px; clear: both;}
.map dd .p2 a { color:@sub_color;font-weight:bold;  overflow:hidden; white-space:nowrap; text-overflow:ellipsis;transition: 0.3s;}
.map dd .p2 a:hover{ color:#f00;}
.map dd div { clear:both;overflow: hidden; padding:0px 20px 0 10px;}
.map dd div a{  color:#333; float:left; padding: 0 10px 0 10px;margin-top: 1px;transition: 0.3s;}
.map dd div a:hover{ color:#003e7e; background: #000; color: #fff;}
 
 @media (max-width: 768px) {
.map dt {width: 15%;}
.map dd { width: 85%;}
}
@media (max-width: 640px) {
.map dt {width: 20%;}
.map dd { width: 80%;}
}
@media (max-width: 400px) {
.map dt {width: 100%;}
.map dd { width: 100%;}
}
 
 
 
 
 
 
